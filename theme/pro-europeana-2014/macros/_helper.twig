{% macro parentPath(parents) %}
	{% set parentRoot = paths.root %}
	{% for parent in parents|reverse %}
		{% set parentRoot = parentRoot~parent.group.slug~"/" %}
	{% endfor %}
	{{ parentRoot }}
{% endmacro %}
{% macro parentlink(parents, child) %}
	{{ _self.parentPath(parents) }}{{ child.slug }}
{% endmacro %}